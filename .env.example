# Configurações do Banco de Dados
# Para H2 (Desenvolvimento)
SPRING_DATASOURCE_URL=jdbc:h2:mem:insanosdb
SPRING_DATASOURCE_USERNAME=sa
SPRING_DATASOURCE_PASSWORD=

# Para PostgreSQL (Produção)
# SPRING_DATASOURCE_URL=jdbc:postgresql://localhost:5432/insanosdb
# SPRING_DATASOURCE_USERNAME=postgres
# SPRING_DATASOURCE_PASSWORD=your_password

# Configurações JWT
JWT_SECRET=insanos-secret-key-very-secure-and-long-at-least-256-bits-for-hs256-algorithm
JWT_EXPIRATION=86400000

# Configurações do Servidor
SERVER_PORT=8080

# CORS - Origens permitidas (separadas por vírgula)
CORS_ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173

